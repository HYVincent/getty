    <developers>
        <developer>
            <id>getty</id>
            <name>gogym</name>
            <email>189155278@qq.com</email>
            <roles>
                <role>getty作者</role>
            </roles>
            <url></url>
            <timezone>GMT+8</timezone>
            <properties>
                <picUrl></picUrl>
            </properties>
            <organization>getty</organization>
            <organizationUrl>https://gitee.com/getty</organizationUrl>
        </developer>
    </developers>


</project>